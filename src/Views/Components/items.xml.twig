<DetalleServicio>
  {% for item in line_items %}
  <LineaDetalle>
    <NumeroLinea>{{ loop.index }}</NumeroLinea>
    {% if item.customs_tariff_code %}
    <PartidaArancelaria>{{ item.customs_tariff_code }}</PartidaArancelaria>
    {% endif %}
    <CodigoCABYS>{{ item.cabys_code }}</CodigoCABYS>
    <CodigoComercial>
      <Tipo>04</Tipo>
      <Codigo>1</Codigo>
    </CodigoComercial>
    <Cantidad>{{ item.quantity }}</Cantidad>
    <UnidadMedida>Sp</UnidadMedida>
    <TipoTransaccion>01</TipoTransaccion>
    <Detalle>{{ item.description }}</Detalle>
    <PrecioUnitario>{{ item.unit_price }}</PrecioUnitario>
    <MontoTotal>50.00000</MontoTotal>
    <SubTotal>50.00000</SubTotal>
    <BaseImponible>50.00000</BaseImponible>
    {% include 'Components/taxes.xml.twig' with { 'node': 'Impuesto','tax':
    item.tax} %}
    <ImpuestoAsumidoEmisorFabrica>0</ImpuestoAsumidoEmisorFabrica>
    <ImpuestoNeto>6.50000</ImpuestoNeto>
    <MontoTotalLinea>56.50000</MontoTotalLinea>
  </LineaDetalle>
  {% endfor %}
</DetalleServicio>
